# React App Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_SOCKET_URL=http://localhost:5000

# Environment
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_COMPLIANCE_CHECKS=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=true

# External Services
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
REACT_APP_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# Compliance Settings
REACT_APP_DEFAULT_AGE_VERIFICATION_REQUIRED=true
REACT_APP_DEFAULT_CONSENT_REQUIRED=true
REACT_APP_MAX_MESSAGE_LENGTH=1600
REACT_APP_MAX_MEDIA_FILES=5

# UI Configuration
REACT_APP_THEME=light
REACT_APP_PRIMARY_COLOR=#2e7d32
REACT_APP_SECONDARY_COLOR=#ff6f00
